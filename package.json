{
  "name": "asset-list",
  "version": "0.0.0",
  "description": "A quick app to list the assets in my CDN ",
  "main": "null",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src test --max-warnings 0 && prettier src test json --check && pnpm run spellcheck",
    "prespellcheck": "ts-node src/prepSpelling.ts",
    "spellcheck": "cspell temp/**/*.json --config config/.cspell.js",
    "postspellcheck": "rm -rf temp",
    "test": "ts-mocha -u tdd 'test/**/*.spec.ts' -exclude test/cdn.spec.ts --recursive --timeout 10000",
    "test:cdn": "ts-mocha -u tdd 'test/cdn.spec.ts' --timeout 10000",
    "count": "node prod/counts.js",
    "list-different": "node prod/listDifferent.js",
    "audit-outfits": "node prod/auditOutfits.js",
    "picrew": "node prod/picrew.js"
  },
  "engines": {
    "node": "20",
    "pnpm": "8"
  },
  "keywords": [
    "assets",
    "json"
  ],
  "author": "Naomi Carrigan",
  "license": "SEE LICENSE IN https://docs.nhcarrigan.com/#/license",
  "devDependencies": {
    "@nhcarrigan/eslint-config": "1.1.3",
    "@nhcarrigan/prettier-config": "1.0.1",
    "@nhcarrigan/typescript-config": "1.0.1",
    "@types/chai": "4.3.6",
    "@types/mocha": "10.0.1",
    "@types/node": "20.6.2",
    "@types/string-similarity": "4.0.0",
    "@types/xml2js": "0.4.11",
    "chai": "4.3.8",
    "cspell": "7.3.6",
    "eslint": "8.49.0",
    "mocha": "10.2.0",
    "prettier": "2.8.8",
    "ts-mocha": "10.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "xml2js": "0.6.2"
  },
  "dependencies": {
    "string-similarity": "4.0.4",
    "winston": "3.10.0"
  }
}